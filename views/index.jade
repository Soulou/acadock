doctype 5
html(lang="fr")
  head
    title Acadock
    != js("application")
    != css("application")

  body
    h1 Acadock

    ul#containers
      each container in containers
        li.container
          = container.id
        li.container
          = container.image
        li.container
          = container.created
        li.container
          = container.status

